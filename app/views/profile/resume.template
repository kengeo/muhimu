  <div class="row">
    <div class="col-md-12">
      <div class="row">
        <div class="col-md-6 col-sm-12">
        <h1 class="rs">{{basics.name}} <br><small>{{basics.location.city}}</small><h1>
		<h2 class="small"></h2>
        </div>
        <div class="col-md-6 col-sm-12">
        <!--<img class="media-object img-circle pull-right" data-src="holder.js/64x64" alt="64x64" src="{{basics.gravatar}}" style="width: 100px; height: 100px; margin-top: 20px;">-->

        </div>
        </div>
      <hr style="color: #ccc; height: 4px;" />
      <section class="row">
        <header class="col-md-3">
          <h3>About</h3>
        </header>
        <div class="col-md-9">
          <p>{{basics.summary}}</p>
          {{#basics.phone}}
          <strong>Phone:</strong> {{basics.phone}} <br />
          {{/basics.phone}}
          {{#basics.email}}
          <strong>Email:</strong> {{basics.email}}<br />
          {{/basics.email}}
          {{#basics.profiles.length}}
          <section id="profiles">
            <div class="row">
              {{#basics.profiles}}
              <div class="col-sm-6">
                {{#network}}
                <strong class="network">
                  {{.}}
                </strong>
                {{/network}}
                {{#username}}
                <div class="username">
                  {{#url}}
                  <div class="url">
                    <a target="_blank" href="{{url}}">{{username}}</a>
                  </div>
                  {{/url}}
                  {{^url}}
                    {{username}}
                  {{/url}}
                </div>
                {{/username}}
                {{^username}}
                  {{#url}}
                  <div class="url">
                    <a target="_blank" href="{{url}}">{{url}}</a>
                  </div>
                  {{/url}}
                {{/username}}
              </div>
              {{/basics.profiles}}
            </div>
          </section>
          {{/basics.profiles.length}}
        </div>
      </section>
      {{#work.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>Work Experience</h3>
        </header>
        <div class="col-md-9">
          <div class="skills">
            {{#work}}
              <h3>{{company}}<span class="date pull-right">{{startDateYear}} - {{endDateYear}}</span></h3>
              <strong>{{position}}</strong>
              {{#website}}
                <div class="website pull-right">
                  <a target="_blank" href="{{.}}">{{.}}</a>
                </div>
              {{/website}}
              <p>{{summary}}</p>
              <ul>
                {{#highlights}}
                <li>{{.}}</li>
                {{/highlights}}
              </ul>
            <hr />{{/work}}
          </div>
        </div>
      </section>
      {{/work.length}}

      {{#education.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>Education</h3>
        </header>
        <div class="col-md-9">
          <div class="skills">
            {{#education}}
            <h3>{{institution}}<span class="date pull-right">{{startDateYear}} - {{endDateYear}}</span></h3>
            <strong>{{educationDetail}}</strong>
            <p>{{summary}}</p>
            {{#courses.length}}
            <ul class="courses">
              {{#courses}}
              <li>{{.}}</li>
              {{/courses}}
            </ul>
            {{/courses.length}}
            <hr />
            {{/education}}
          </div>
        </div>
      </section>
      {{/education.length}}
      {{#volunteer.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>Volunteer</h3>
        </header>
        <div class="col-md-9">
          <div class="skills">
            {{#volunteer}}
            <h3>{{organization}}<span class="date pull-right">{{startDateYear}} - {{endDateYear}}</span></h3>
            <strong>{{position}}</strong>
            {{#website}}
              <div class="website pull-right">
                <a target="_blank" href="{{.}}">{{.}}</a>
              </div>
            {{/website}}
            <p>{{summary}}</p>
            <ul>
              {{#highlights}}
              <li>{{.}}</li>
              {{/highlights}}
            </ul>
            <hr />
            {{/volunteer}}
          </div>
        </div>
      </section>
      {{/volunteer.length}}
      {{#skills.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>Skills</h3>
        </header>
        <div class="col-md-9">
          <div class="skills">
            {{#skills}}
            <ul>
              <li>
                <h4>{{name}} <span class="pull-right">{{level}}</span></h4>
                <ul>
                  {{#keywords}}
                  <li>{{.}}</li>
                  {{/keywords}}
                </ul>
              </li>
            </ul>
            {{/skills}}
          </div>
        </div>
      </section>
      {{/skills.length}}
      {{#languages.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>Languages</h3>
        </header>
        <div class="col-md-9">
          <div class="skills">
            {{#languages}}
            <ul>
              <li>
                <h4>{{language}} <span class="pull-right">{{fluency}}</span></h4>
              </li>
            </ul>
            {{/languages}}
          </div>
        </div>
      </section>
      {{/languages.length}}
      {{#references.length}}
      <section class="row">
        <header class="col-md-3">
          <h3>References</h3>
        </header>
        <div class="col-md-9">
          <div class="row-fluid skills">
            {{#references}}

            <p>{{reference}} -
              <strong>{{name}}</strong>
            </p>
            {{/references}}
          </div>
        </div>
      </section>
      {{/references.length}}

    </div>
  </div>
